; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tinybox"
config/version="13.1"
run/main_scene="res://data/scene/MainScene.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Tinybox"
config/features=PackedStringArray("4.5", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="uid://ccl1h2rnq8f85"
config/windows_native_icon="res://icon.ico"
boot_splash/windows_native_icon="res://icon.ico"

[autoload]

UIHandler="*res://src/ui/UIHandler.gd"
Global="*res://src/Global.gd"
PersistentScene="*res://data/scene/ui/PersistentScene.tscn"
JsonHandler="*res://data/scene/autoload/JSONHandler.tscn"
MusicHandler="*res://data/scene/MusicHandler.tscn"
UserPreferences="*res://src/UserPreferences.gd"
SpawnableObjects="*res://src/SpawnableObjects.gd"
CommandHandler="*res://src/CommandHandler.gd"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=2
file_logging/use_filter=true
file_logging/fullsize=true
file_logging/show_image=false
file_logging/image="res://data/textures/load_text.png"

[display]

window/size/viewport_width=1200
window/size/viewport_height=720
window/dpi/allow_hidpi=false

[editor]

script/templates_search_path="res://data/script_templates"

[editor_plugins]

enabled=PackedStringArray("res://addons/LANServerBroadcast/plugin.cfg", "res://addons/smoothing/plugin.cfg")

[file_customization]

folder_colors={
"res://data/": "green",
"res://src/": "orange"
}

[gui]

timers/tooltip_delay_sec=0.25
theme/custom="res://default_theme.tres"

