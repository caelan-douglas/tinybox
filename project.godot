[layer_names]

3d_physics/layer_1="all"
3d_physics/layer_2="player"
3d_physics/layer_3="player_areas"
3d_physics/layer_4="camera"
3d_physics/layer_5="extra"
3d_physics/layer_8="clayball"
3d_physics/layer_9="editor_selection_area"
3d_physics/layer_10="water_only_collider"

[physics]

common/physics_ticks_per_second=45
3d/physics_engine="GodotPhysics3D"
3d/sleep_threshold_linear=0.3
3d/sleep_threshold_angular=0.5
3d/time_before_sleep=0.4

[rendering]

textures/vram_compression/import_etc2_astc=true
reflections/reflection_atlas/reflection_count=4
global_illumination/gi/use_half_resolution=true
anti_aliasing/quality/msaa_3d=1
global_illumination/sdfgi/probe_ray_count=0
global_illumination/sdfgi/frames_to_update_lights=4
